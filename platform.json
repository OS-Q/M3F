{
    "name": "P71",
    "title": "Platform P71",
    "version": "1.2.4",
    "description": "Kendryte K210 is an AI capable RISCV64 dual core SoC.",
    "homepage": "https://kendryte.com/",
    "license": "Apache-2.0",
    "engines": {
        "platformio": "^5"
    },
    "keywords": [
        "OS-Q",
        "dev-platform",
        "Kendryte",
        "SoC",
        "RISC-V",
        "64bit",
        "AI"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/OS-Q/P71.git"
    },
    "frameworks": {
        "kendryte-standalone-sdk": {
            "package": "N71",
            "script": "builder/frameworks/kendryte-standalone-sdk.py",
            "description": "Kendryte Standalone SDK without OS support",
            "homepage": "https://github.com/kendryte/kendryte-standalone-sdk",
            "title": "Kendryte Standalone SDK"
        },
        "kendryte-freertos-sdk": {
            "package": "F71",
            "script": "builder/frameworks/kendryte-freertos-sdk.py",
            "description": "Kendryte SDK with FreeRTOS support",
            "homepage": "https://github.com/kendryte/kendryte-freertos-sdk",
            "title": "Kendryte FreeRTOS SDK"
        },
        "arduino":{
            "package": "A71",
            "script": "builder/frameworks/arduino.py"
        }
    },
    "packages": {
        "toolchain-kendryte210": {
            "type": "toolchain",
            "owner": "platformio",
            "version": "~8.2.0"
        },
        "A71": {
            "type": "framework",
            "optional": true,
            "version": "https://github.com/OS-Q/A71.git"
        },
        "N71": {
            "type": "framework",
            "optional": true,
            "version": "https://github.com/OS-Q/N71.git"
        },
        "F71": {
            "type": "framework",
            "optional": true,
            "version": "https://github.com/OS-Q/F71.git"
        },
        "tool-kflash-kendryte210": {
            "type": "uploader",
            "optional": true,
            "owner": "platformio",
            "version": "~0.9.1"
        },
        "tool-openocd-kendryte": {
            "type": "uploader",
            "optional": true,
            "owner": "platformio",
            "version": "^1.203.0"
        }
    }
}
